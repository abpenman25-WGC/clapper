{
  "name": "@aitube/colors",
  "module": "index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "version": "0.2.4",
  "description": "Various color settings and utilities for AiTube",
  "scripts": {
    "build": "tsc && pnpm run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "postbuild": "rimraf tsconfig.types.tsbuildinfo && pnpm run build:declaration",
    "publish": "npm publish --access public",
    "update": "rm -Rf node_modules && rm pnpm-lock.yaml && pnpm i && pnpm run build"
  },
  "devDependencies": {
    "bun-types": "latest",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "5.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jbilcke-hf/aitube-colors.git"
  },
  "keywords": [
    "OpenClap",
    "AiTube"
  ],
  "author": "Julian Bilcke",
  "license": "MIT",
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/**/*.d.ts"
  ]
}
