{
  "name": "@aitube/clapper-monorepo",
  "version": "0.2.4",
  "description": "A monorepo for the Clapper project. Individual packages are in the packages directory.",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --filter @aitube/clapper dev",
    "start": "pnpm --filter @aitube/clapper start",
    "start:prod": "pnpm --filter @aitube/clapper start:prod",
    "build": "pnpm run build:all",
    "build:all": "pnpm run build:clap && pnpm run build:timeline && pnpm run build:client && pnpm run build:io && pnpm run build:colors && pnpm run build:engine && pnpm run build:broadway && pnpm run build:clapper-services && pnpm run build:app",
    "build:clap": "pnpm --filter @aitube/clap build",
    "build:timeline": "pnpm --filter @aitube/timeline build",
    "build:client": "pnpm --filter @aitube/client build",
    "build:io": "pnpm --filter @aitube/io build",
    "build:colors": "pnpm --filter @aitube/colors build",
    "build:engine": "pnpm --filter @aitube/engine build",
    "build:broadway": "pnpm --filter @aitube/broadway build",
    "build:clapper-services": "pnpm --filter @aitube/clapper-services build",
    "build:app": "pnpm --filter @aitube/clapper build",
    "test": "pnpm run test:all",
    "test:all": "pnpm --filter @aitube/clap test && pnpm --filter @aitube/timeline test && pnpm --filter @aitube/client test && pnpm --filter @aitube/io test && pnpm --filter @aitube/colors test && pnpm --filter @aitube/engine test && pnpm --filter @aitube/broadway test && pnpm --filter @aitube/clapper-services test && pnpm --filter @aitube/clapper test",
    "format": "prettier --write ."
  },
  "trustedDependencies": [
    "@aitube/clapper",
    "onnxruntime-node",
    "protobufjs"
  ],
  "workspaces": [
    "packages/clap",
    "packages/timeline",
    "packages/client",
    "packages/io",
    "packages/colors",
    "packages/engine",
    "packages/broadway",
    "packages/clapper-services",
    "packages/app"
  ],
  "dependencies": {
    "@swc/helpers": "^0.5.18",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "caniuse-lite": "^1.0.30001764",
    "mediainfo.js": "^0.3.6",
    "next": "14.2.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolve": "^1.22.11",
    "styled-jsx": "^5.1.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/three": "^0.182.0",
    "@webgpu/types": "^0.1.65",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2"
  }
}